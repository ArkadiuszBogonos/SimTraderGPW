<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/fr_wallet_title_background"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:background="@color/colorDelicateGray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fr_wallet_label_wallet_value_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Wartość porfela:"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider7" />

    <TextView
        android:id="@+id/fr_wallet_wallet_value_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text=" 22065,60"
        android:textColor="@color/colorTrendingUp"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/fr_wallet_label_wallet_value_tv"
        app:layout_constraintStart_toEndOf="@+id/fr_wallet_label_wallet_value_tv"
        app:layout_constraintTop_toTopOf="@+id/fr_wallet_label_wallet_value_tv" />

    <TextView
        android:id="@+id/fr_wallet_label_money_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Środki pieniężne:"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fr_wallet_label_wallet_value_tv" />

    <TextView
        android:id="@+id/fr_wallet_label_stocks_value_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Wartość papierów:"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fr_wallet_label_money_tv" />

    <TextView
        android:id="@+id/fr_wallet_money_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="10000,60"
        android:textColor="@color/colorTrendingUp"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/fr_wallet_label_money_tv"
        app:layout_constraintStart_toEndOf="@+id/fr_wallet_label_money_tv"
        app:layout_constraintTop_toTopOf="@+id/fr_wallet_label_money_tv" />

    <TextView
        android:id="@+id/fr_wallet_stocks_value_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="0"
        android:textColor="@color/colorTrendingUp"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/fr_wallet_label_stocks_value_tv"
        app:layout_constraintStart_toEndOf="@+id/fr_wallet_label_stocks_value_tv"
        app:layout_constraintTop_toTopOf="@+id/fr_wallet_label_stocks_value_tv" />

    <View
        android:id="@+id/divider6"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fr_wallet_label_loan_tv" />

    <TextView
        android:id="@+id/fr_wallet_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Szczegóły portfela"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/fr_wallet_title_background"
        app:layout_constraintEnd_toEndOf="@+id/fr_wallet_title_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fr_wallet_owned_stocks_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Posiadane papiery"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider6" />

    <TextView
        android:id="@+id/fr_wallet_login_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="KIERSON"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fr_wallet_title_background" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="344"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/fr_wallet_label_wallet_value_tv"
        app:layout_constraintStart_toEndOf="@+id/fr_wallet_login_tv"
        app:layout_constraintTop_toTopOf="@+id/fr_wallet_login_tv"
        app:layout_constraintVertical_bias="0.19999999" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:textColor="@color/colorTrendingUp"
        app:layout_constraintBottom_toBottomOf="@+id/textView12"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/textView12" />

    <View
        android:id="@+id/view2"
        android:layout_width="16dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:background="@drawable/ic_baseline_arrow_drop_up_green_24"
        app:layout_constraintBottom_toBottomOf="@+id/textView13"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/textView13"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/fr_wallet_currency_sufix_tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:text="PLN"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/fr_wallet_wallet_value_tv"
        app:layout_constraintStart_toEndOf="@+id/fr_wallet_wallet_value_tv"
        app:layout_constraintTop_toTopOf="@+id/fr_wallet_wallet_value_tv"
        app:layout_constraintVertical_bias="0.8" />

    <TextView
        android:id="@+id/fr_wallet_currency_sufix_tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:text="PLN"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/fr_wallet_money_tv"
        app:layout_constraintStart_toEndOf="@+id/fr_wallet_money_tv"
        app:layout_constraintTop_toTopOf="@+id/fr_wallet_money_tv"
        app:layout_constraintVertical_bias="0.8" />

    <TextView
        android:id="@+id/fr_wallet_currency_sufix_tv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:text="PLN"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/fr_wallet_stocks_value_tv"
        app:layout_constraintStart_toEndOf="@+id/fr_wallet_stocks_value_tv"
        app:layout_constraintTop_toTopOf="@+id/fr_wallet_stocks_value_tv"
        app:layout_constraintVertical_bias="0.8" />

    <TextView
        android:id="@+id/fr_wallet_label_loan_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Kredyty:"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fr_wallet_label_stocks_value_tv" />

    <TextView
        android:id="@+id/fr_wallet_loan_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="2000,00"
        android:textColor="@color/colorTrendingDown"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/fr_wallet_label_loan_tv"
        app:layout_constraintStart_toEndOf="@+id/fr_wallet_label_loan_tv"
        app:layout_constraintTop_toTopOf="@+id/fr_wallet_label_loan_tv" />

    <TextView
        android:id="@+id/fr_wallet_currency_sufix_tv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PLN"
        android:textColor="@color/colorTrendingDown"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/fr_wallet_loan_tv"
        app:layout_constraintStart_toEndOf="@+id/fr_wallet_loan_tv"
        app:layout_constraintTop_toTopOf="@+id/fr_wallet_loan_tv"
        app:layout_constraintVertical_bias="0.8" />

    <View
        android:id="@+id/divider7"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintTop_toBottomOf="@+id/fr_wallet_login_tv"
        tools:layout_editor_absoluteX="1dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:background="@color/colorDelicateGray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fr_wallet_owned_stocks_tv">

        <TextView
            android:id="@+id/textView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="Nazwa"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/guideline20"
            app:layout_constraintHorizontal_bias="0.508"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.35" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.37" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="Ticker"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/guideline21"
            app:layout_constraintStart_toStartOf="@+id/guideline16"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.53" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.54" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="Ilość"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/guideline22"
            app:layout_constraintStart_toStartOf="@+id/guideline18"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.72" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.73" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="Szacowana wartość"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/guideline19"
            app:layout_constraintStart_toStartOf="@+id/guideline17"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.98" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2" />

</androidx.constraintlayout.widget.ConstraintLayout>